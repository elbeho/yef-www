[%
 USE YAML;
 confs = YAML.undumpfile("conferences.yml");

 conferences = [] ;
 FOR item = confs ; 
     NEXT IF item.begin.match( 'xx' );
     NEXT IF item.name != 'YAPC Europe';
 
     item.year = item.begin.match( '(\d\d\d\d)$' ).0;
     item.anchor = item.begin.match('(\d\d\d\d)$').item.city;
     conferences.push(item);
 END
%]
